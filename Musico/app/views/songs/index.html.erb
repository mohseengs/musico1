<turbo-frame id="main">
  <div class="songs m-3">
  <h4 class="m3 mt-5 mb-5">Songs</h4>
    <div class="row">
    <% @songs.each do |song| %>
        <div class="col-3">
          <%= render partial: "shared/song_head" , locals: { song: song } %>
        </div>
    <% end %>
    </div>
  </div>
</turbo-frame>
 

