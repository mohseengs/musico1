<div class="row p-5 justify-content-center">
    <div class="col-12 text-center">
        <h5>New Song</h5>
    </div>
    <div class="col-8">
        <%= render 'song_upload_form', song: @song %>
    </div>
</div>


<script>

    $(document).ready(function() {
        $('#song_search_query').on('change', function() {
            var query = $(this).val();
          
            $.ajax({
                url: '/search_user/' + query + '.js',
                type: 'GET'
            });

        });
    });

</script>
