<div class="col-12">
<div class="row m-2 mt-5" >
     <% if songs && !songs.empty? %>
        <div class="col-12">
            <h3>songs</h3>
        </div>
        <% songs.each do |song| %>
            <div class="col-2">
                <%= render partial: "shared/song_head" , locals: { song: song } %>
            </div>
        <% end %>
    <% end %>
</div>

<div class="row m-2 mt-5" >
     <% if users && !users.empty? %>
        <div class="col-12">
            <h3>Artists</h3>
        </div>
        <% users.each do |user| %>
            <div class="col-2">
                <%= render partial: "shared/artist_head" , locals: { artist: user } %>
            </div>
        <% end %>
    <% end %>
</div>

<div class="row m-2 mt-5" >
     <% if playlists && !playlists.empty? %>
        <div class="col-12">
            <h3>Playlists</h3>
        </div>
        <% playlists.each do |playlist| %>
            <div class="col-2">
                <%= render partial: "shared/playlist_head" , locals: { playlist: playlist } %>
            </div>
        <% end %>
    <% end %>
</div>

</div>
